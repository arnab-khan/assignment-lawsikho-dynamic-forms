[
    {
        "label": "Profile Picture",
        "formControlName": "profilePicture",
        "type": "file",
        "sequence": 1,
        "validation": {
            "mandatory": {
                "value": false,
                "errorMessage": ""
            }
        }
    },
    {
        "label": "Address",
        "formControlName": "address",
        "type": "input",
        "sequence": 2,
        "validation": {
            "mandatory": {
                "value": true,
                "errorMessage": "Address is required"
            },
            "pattern": {
                "value": "^[a-zA-Z0-9\\s,.-]{5,100}$",
                "errorMessage": "Address must be between 5 and 100 characters"
            }
        }
    },
    {
        "label": "City",
        "formControlName": "city",
        "type": "input",
        "sequence": 3,
        "validation": {
            "mandatory": {
                "value": true,
                "errorMessage": "City is required"
            },
            "pattern": {
                "value": "^[a-zA-Z\\s]{2,50}$",
                "errorMessage": "City must be between 2 and 50 characters and contain only letters and spaces"
            }
        }
    },
    {
        "label": "State",
        "formControlName": "state",
        "type": "input",
        "sequence": 4,
        "validation": {
            "mandatory": {
                "value": true,
                "errorMessage": "State is required"
            },
            "pattern": {
                "value": "^[a-zA-Z\\s]{2,50}$",
                "errorMessage": "State must be between 2 and 50 characters and contain only letters and spaces"
            }
        }
    },
    {
        "label": "Pincode",
        "formControlName": "pincode",
        "type": "input",
        "sequence": 5,
        "validation": {
            "mandatory": {
                "value": true,
                "errorMessage": "Pincode is required"
            },
            "pattern": {
                "value": "^[0-9]{6}$",
                "errorMessage": "Pincode must be 6 digits"
            }
        }
    },
    {
        "label": "Country",
        "formControlName": "country",
        "type": "select",
        "options": [
            "Country 1",
            "Country 2",
            "Country 3",
            "Country 4",
            "Country 5"
        ],
        "sequence": 6,
        "validation": {
            "mandatory": {
                "value": true,
                "errorMessage": "Country is required"
            }
        }
    }
]